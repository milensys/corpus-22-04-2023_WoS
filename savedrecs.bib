@article{ WOS:000618346000005,
Author = {Yu, Chaoran and Helwig, Ernest Johann},
Title = {Artificial intelligence in gastric cancer: a translational narrative
   review},
Journal = {ANNALS OF TRANSLATIONAL MEDICINE},
