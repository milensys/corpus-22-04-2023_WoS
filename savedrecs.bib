@article{ WOS:000580815300001,
Author = {Lee, Kwang-Sig and Ahn, Ki Hoon},
Title = {Application of Artificial Intelligence in Early Diagnosis of Spontaneous
   Preterm Labor and Birth},
Journal = {DIAGNOSTICS},
