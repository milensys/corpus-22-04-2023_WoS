@article{ WOS:000478140300001,
Author = {Zimmermann, Albrecht},
Title = {Method evaluation, parameterization, and result validation in
   unsupervised data mining: A critical survey},
Journal = {WILEY INTERDISCIPLINARY REVIEWS-DATA MINING AND KNOWLEDGE DISCOVERY},
Year = {2020},
