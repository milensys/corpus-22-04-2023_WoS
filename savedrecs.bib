Author = {Saad, Mohamad Hanif Md and Hamdan, Nurul Maisarah and Sarker, Mahidur R.},
Title = {State of the Art of Urban Smart Vertical Farming Automation System:
   Advanced Topologies, Issues and Recommendations},
Journal = {ELECTRONICS},
