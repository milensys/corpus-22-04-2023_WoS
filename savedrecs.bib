@article{ WOS:000539634600001,
Author = {Bayram, Mustafa and Springer, Simon and Garvey, Colin K. and Vural,
   Ozdemir},
Title = {COVID-19 Digital Health Innovation Policy: A Portal to Alternative
   Futures in the Making},
Journal = {OMICS-A JOURNAL OF INTEGRATIVE BIOLOGY},
