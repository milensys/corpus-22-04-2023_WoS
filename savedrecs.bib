@article{ WOS:000788276000001,
Author = {Deng, Tianhu and Zhang, Keren and Shen, Zuo-Jun (Max)},
Title = {A systematic review of a digital twin city: A new pattern of urban
   governance toward smart cities},
Journal = {JOURNAL OF MANAGEMENT SCIENCE AND ENGINEERING},
