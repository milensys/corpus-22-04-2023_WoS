@article{ WOS:000685394200001,
Author = {Subrahmanya, Sri Venkat Gunturi and Shetty, Dasharathraj K. and Patil,
   Vathsala and Hameed, B. M. Zeeshan and Paul, Rahul and Smriti, Komal and
   Naik, Nithesh and Somani, Bhaskar K.},
Title = {The role of data science in healthcare advancements: applications,
   benefits, and future prospects},
Journal = {IRISH JOURNAL OF MEDICAL SCIENCE},
