@article{ WOS:000663301100002,
Author = {Derathe, Arthur and Reche, Fabian and Jannin, Pierre and Moreau-Gaudry,
   Alexandre and Gibaud, Bernard and Voros, Sandrine},
Title = {Explaining a model predicting quality of surgical practice: a first
   presentation to and review by clinical experts},
Journal = {INTERNATIONAL JOURNAL OF COMPUTER ASSISTED RADIOLOGY AND SURGERY},
Year = {2021},
