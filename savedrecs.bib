@article{ WOS:000345734700045,
Author = {Araghi, Sahar and Khosravi, Abbas and Creighton, Douglas},
Title = {A review on computational intelligence methods for controlling traffic
   signal timing},
Journal = {EXPERT SYSTEMS WITH APPLICATIONS},
Year = {2015},
Volume = {42},
