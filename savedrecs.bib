ISSN = {1832-9101},
Keywords = {Reza Negarestani; Nick Land; Intelligence and Spirit; Crypto-current;
   Bitcoin; cryptocurrency; antihumanism; neorationalism; Hegel; Kant;
   artificial intelligence; Sellars; Brandom; nihilism; blockchain;
   accelerationism},
