@article{ WOS:000543394800155,
Author = {Shi, Wenzhong and Zhang, Min and Zhang, Rui and Chen, Shanxiong and
   Zhan, Zhao},
Title = {Change Detection Based on Artificial Intelligence: State-of-the-Art and
   Challenges},
