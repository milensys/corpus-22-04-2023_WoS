@article{ WOS:000909574400001,
Author = {Malik, Sumbal and Khan, Manzoor Ahmed and El-Sayed, Hesham and Khan,
   Jalal and Ullah, Obaid},
Title = {How Do Autonomous Vehicles Decide?},
