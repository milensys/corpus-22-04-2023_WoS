@article{ WOS:000691223200001,
Author = {Munawar, Hafiz Suliman and Hammad, Ahmed W. A. and Haddad, Assed and
   Pereira Soares, Carlos Alberto and Waller, S. Travis},
Title = {Image-Based Crack Detection Methods: A Review},
